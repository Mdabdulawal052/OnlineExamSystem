@model OnlineExamSystem.Models.ParticipantCreateForView
@{
    Layout = null;
}

<div class="">
    <table class="table  table-hover mt-5 pt-5">
        <thead class="table-primary">
        <tr>
            <th>Name</th>
            <th>ContactNo</th>
            <th>Email</th>
            <th>City</th>
            <th>Country</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        @if (Model != null && Model.ParticipantListItems != null && Model.ParticipantListItems.Count > 0)
        {
            foreach (var org in Model.ParticipantListItems)
            {
                <tr>
                    <td>@org.Name</td>
                    <td>@org.ContactNo</td>
                    <td>@org.Email</td>
                    <td>@org.City</td>
                    <td>@org.Country</td>
                    <td>
                        @Html.ActionLink("Details", "ParticipantDetails", new { id = org.Id })
                    </td>
                </tr>
            }
        }


        </tbody>
    </table>
</div>
